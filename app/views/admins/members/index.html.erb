<!-- 会員検索フォーム -->
<%= render "members/members/search_form", { search_params: @search_params, admin_flag: true } %>

<h2>メンバー一覧</h2>

<% if @members.exists? %>
  <div class="row" id="member_lists">
    <!-- メンバー一覧 -->
    <%= render "admins/members/member_lists", { members: @members }  %>
  </div>
<% else %>
  <p class="NoCount">メンバーはいません</p>
<% end %>

<!-- もっと見るボタン -->
<%= render "layouts/and_more_button", { model: @members } %>