<h2>ブロックリスト</h2>

<% if @members.exists? %>
  <div class="row" id="blocker_lists">
    <!-- ブロックリスト一覧 -->
    <%= render "members/members/member_lists", { members: @members }  %>
  </div>
<% else %>
  <p class="NoCount">ブロックしている人はいません</p>
<% end %>

<!-- もっと見るボタン -->
<%= render "layouts/and_more_button", { model: @members } %>