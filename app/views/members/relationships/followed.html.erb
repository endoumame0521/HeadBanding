<h2>フォロワー</h2>

<% if @members.exists? %>
  <div class="row" id="followed_lists">
    <!-- フォロー一覧 -->
    <%= render "members/members/member_lists", { members: @members }  %>
  </div>
<% else %>
  <p class="NoCount">フォロワーはいません</p>
<% end %>

<!-- もっと見るボタン -->
<%= render "layouts/and_more_button", { model: @members } %>