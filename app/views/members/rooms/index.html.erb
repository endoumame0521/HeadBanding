<h2>メッセージ一覧</h2>

<% if @entry_rooms.exists? %>
  <div class="row" id="room_lists">
    <!-- メッセージルーム一覧 -->
    <%= render "members/rooms/room_lists", { entry_rooms: @entry_rooms }  %>
  </div>
<% else %>
  <p class="NoCount">メッセージはありません</p>
<% end %>

<!-- もっと見るボタン -->
<%= render "layouts/and_more_button", { model: @entry_rooms } %>